<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alzyfunnyapp">

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".GameActivity"></activity>

        <activity android:name=".PopaboutActivity"
                  android:theme="@style/AppTheme.PopMe"/>

        <activity android:name=".PopcontactActivity"
                  android:theme="@style/AppTheme.PopMe"/>

        <activity android:name=".PopuserActivity"
                  android:theme="@style/AppTheme.PopMe"/>

        <meta-data
            android:name="luckiest_guy"
            android:resource="@font/luckiest_guy" />


    </application>

</manifest>